<?xml version="1.0" encoding="utf-8"?>
<odoo>
  <record id="palletways_ext_company_view" model="ir.ui.view">
    <field name="name">palletways.ext.company.view</field>
    <field name="model">res.company</field>
    <field name="inherit_id" ref="base.view_company_form"/>
    <field name="arch" type="xml">
      <xpath expr="//page[last()]" position="after">
        <page string="Palletways Ext">
          <group>
            <group>
              <field name="palletways_depot_code"/>
              <field name="palletways_account_code"/>
            </group>
          </group>
        </page>
      </xpath>
    </field>
  </record>
</odoo>
